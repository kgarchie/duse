{
  "name": "duse",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"nuxt dev\" \"sass --watch scss/main.scss:assets/css/style.css\"",
    "build": "sass src/main.scss:assets/css/style.css && nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "nuxt start",
    "drizzle:generate": "drizzle-kit generate:mysql",
    "drizzle:push": "drizzle-kit push:mysql",
    "migrate": "yarn drizzle:generate && yarn drizzle:push"
  },
  "devDependencies": {
    "@types/node": "^18",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.18.1",
    "nuxt": "^3.4.3",
    "sass": "^1.63.3"
  },
  "dependencies": {
    "drizzle-orm": "^0.26.5",
    "mysql2": "^3.3.4"
  }
}
